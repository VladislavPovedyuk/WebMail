package com.teaminternational.mail.service;

import org.springframework.stereotype.Service;

/**
 * User: Vladislav Povedyuk
 * Date: 05.11.13
 */

/**
 * Class used like a property holder of imap and pop3 protocols which are injected in data.xml file
 */
@Service
public final class PropertyHolder {
    private String imapMailStoreProtocol;
    private String imapHost;
    private String pop3SocketFactoryClass;
    private String pop3SocketFactoryPort;
    private String pop3Port;
    private String pop3Protocol;
    private String pop3Host;

    public String getImapMailStoreProtocol() {
        return imapMailStoreProtocol;
    }

    public void setImapMailStoreProtocol(String imapMailStoreProtocol) {
        this.imapMailStoreProtocol = imapMailStoreProtocol;
    }

    public String getImapHost() {
        return imapHost;
    }

    public void setImapHost(String imapHost) {
        this.imapHost = imapHost;
    }

    public String getPop3SocketFactoryClass() {
        return pop3SocketFactoryClass;
    }

    public void setPop3SocketFactoryClass(String pop3SocketFactoryClass) {
        this.pop3SocketFactoryClass = pop3SocketFactoryClass;
    }

    public String getPop3SocketFactoryPort() {
        return pop3SocketFactoryPort;
    }

    public void setPop3SocketFactoryPort(String pop3SocketFactoryPort) {
        this.pop3SocketFactoryPort = pop3SocketFactoryPort;
    }

    public String getPop3Port() {
        return pop3Port;
    }

    public void setPop3Port(String pop3Port) {
        this.pop3Port = pop3Port;
    }

    public String getPop3Protocol() {
        return pop3Protocol;
    }

    public void setPop3Protocol(String pop3Protocol) {
        this.pop3Protocol = pop3Protocol;
    }

    public String getPop3Host() {
        return pop3Host;
    }

    public void setPop3Host(String pop3Host) {
        this.pop3Host = pop3Host;
    }
}
